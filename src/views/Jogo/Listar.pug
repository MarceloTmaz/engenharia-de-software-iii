include ../template

table 
   
    each i in dados.total
      tr  
        td(colspan='1') 
          h3 Nome: 
        td(colspan='1') 
          h1(id=i.getNome(), name=i.getNome()) #{i.getNome()}
      tr  
        td(colspan='1') 
          h3 Descrição: 
        td(colspan='1') 
          h3(id=i.getDesc(), name=i.getDesc()) #{i.getDesc()}
      tr 

        
      tr 
        td(colspan='1') 
          h3 Opções: 
        td(colspan='1') 
          form(action="/jogo/deletar", method="post")
            input(type="number", id="idR", name="idR", value=i.getId(),  hidden)
            button(type="submit" class="btn btn-outline-secondary") Remover Jogo